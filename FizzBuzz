class Solution(object):
    def fizzBuzz(self, n):
        arr = list()
        for i in range(n):
            if (i + 1) % 3 == 0:
                if (i + 1) % 5 == 0:
                    arr.append("FizzBuzz")
                else:
                    arr.append("Fizz")
            elif (i + 1) % 5 == 0:
                arr.append("Buzz")
            else: 
                arr.append(str(i + 1))
        return arr
